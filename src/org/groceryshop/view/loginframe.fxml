<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.Pane?>
<Pane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity" prefHeight="400.0" prefWidth="457.0" xmlns="http://javafx.com/javafx/8"
      fx:controller="org.groceryshop.controller.LoginController">
    <children>
        <Label layoutX="191.0" layoutY="26.0" prefHeight="92.0" prefWidth="100.0" styleClass="logo"
               stylesheets="@../css/app.css"/>
        <Label layoutX="27.0" layoutY="168.0" prefHeight="34.0" prefWidth="138.0" stylesheets="@../css/app.css"
               text="User Name"/>
        <TextField fx:id="username" layoutX="194.0" layoutY="168.0" prefHeight="34.0" prefWidth="216.0"
                   promptText="User Name" stylesheets="@../css/app.css"/>
        <Label layoutX="27.0" layoutY="216.0" prefHeight="41.0" prefWidth="138.0" stylesheets="@../css/app.css"
               text="Password"/>
        <PasswordField fx:id="userPassword" layoutX="194.0" layoutY="219.0" prefHeight="34.0" prefWidth="216.0"
                       promptText="Password" stylesheets="@../css/app.css"/>
        <Hyperlink layoutX="300.0" layoutY="265.0" prefHeight="23.0" prefWidth="148.0" stylesheets="@../css/app.css"
                   text="Forgot Password"/>
        <Button fx:id="loginButton" layoutX="63.0" layoutY="298.0" mnemonicParsing="false" onAction="#checkLogin"
                prefHeight="64.0" prefWidth="122.0" stylesheets="@../css/app.css" text="Log in"/>
        <Button fx:id="resetButton" layoutX="290.0" layoutY="298.0" mnemonicParsing="false" onAction="#resetForm"
                prefHeight="64.0" prefWidth="122.0" text="Reset"/>
    </children>
</Pane>
